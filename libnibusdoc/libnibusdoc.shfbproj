    <FeedbackEMailAddress>support%40nata-info.ru</FeedbackEMailAddress>
    <CopyrightText>%28c%29 2012 Nata-Info Ltd.</CopyrightText>
    <ContentPlacement>AboveNamespaces</ContentPlacement>
